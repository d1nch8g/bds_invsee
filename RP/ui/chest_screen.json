{
	"namespace": "chest",
	"chest_label": {
		"$new_container_title|default": "$container_title",
		"$text": "$new_container_title"
	},
	"small_chest_screen": {
		"$new_container_title|default": "$container_title",
		"modifications": [
			{
				"array_name": "variables",
				"operation": "insert_back",
				"value": [
					{
						"requires": "($new_container_title = '§o§e§d')",
						"$screen_content": "chest.crystal_chest",
						"$screen_bg_content": "common.screen_background"
					},
					{
					    "requires": "($new_container_title = 'iron_chests.obsidian_chest') or ($new_container_title = 'iron_chests.diamond_chest')",
						"$screen_content": "chest.obsidian_chest",
						"$screen_bg_content": "common.screen_background"
					},
					{
					    "requires": "($new_container_title = 'iron_chests.gold_chest')",
						"$screen_content": "chest.gold_chest",
						"$screen_bg_content": "common.screen_background"
					},
					{
					    "requires": "($new_container_title = 'iron_chests.iron_chest')",
						"$screen_content": "chest.iron_chest",
						"$screen_bg_content": "common.screen_background"	
					},
					{
					    "requires": "($new_container_title = 'iron_chests.copper_chest')",
						"$screen_content": "chest.copper_chest",
						"$screen_bg_content": "common.screen_background"	
					},
					{
					    "requires": "($new_container_title = 'iron_chests.dirt_chest')",
						"$screen_content": "chest.dirt_chest",
						"$screen_bg_content": "common.screen_background"	
					}
				]
			}
		]
	},
	"small_chest_screen@common.inventory_screen_common": {
